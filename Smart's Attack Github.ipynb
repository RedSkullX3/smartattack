    "#Hensel's Lift Program\n",
    "\n",
    "def HenselLift(P, p, prec):\n",
    "    E = P.curve()\n",
    "    Eq = E.change_ring(QQ)          # Lift curve to rational coefficients\n",
    "    Ep = Eq.change_ring(Qp(p, prec))  # Lift curve to Q_p\n",
    "    x_P, y_P = P.xy()\n",
    "\n",
    "    x_lift = ZZ(x_P)\n",
    "    y_lift = ZZ(y_P)\n",
    "\n",
    "    # Define the function F(Y) = Y^2 - (x^3 + a4*x + a6) evaluated at x = x_P\n",
    "    a1, a2, a3, a4, a6 = [ZZ(Eq.a1()), ZZ(Eq.a2()), ZZ(Eq.a3()), ZZ(Eq.a4()), ZZ(Eq.a6())]\n",
    "    rhs = x_lift^3 + a2*x_lift^2 + a4*x_lift + a6\n",
    "    F = lambda y: y^2 + a1*x_lift*y + a3*y - rhs\n",
    "    F_prime = lambda y: 2*y + a1*x_lift + a3\n",
    "\n",
    "    # Check Hensel applicability\n",
    "    if Mod(F(y_lift), p) != 0:\n",
    "        raise ValueError(\"Initial point is not a root mod p.\")\n",
    "    if Mod(F_prime(y_lift), p) == 0:\n",
    "        raise ValueError(\"Derivative is zero mod p. Hensel's Lemma fails.\")\n",
    "\n",
    "    # Begin Newton-Hensel iteration\n",
    "    for i in range(1, prec):\n",
    "        f_val = F(y_lift)\n",
    "        f_prime_val = F_prime(y_lift)\n",
    "\n",
    "        # Compute inverse of derivative mod p^i\n",
    "        try:\n",
    "            u_inv = inverse_mod(f_prime_val, p^i)\n",
    "        except ZeroDivisionError:\n",
    "            raise ValueError(\"Derivative not invertible mod p^%d\" % i)\n",
    "\n",
    "        y_lift = y_lift - u_inv * f_val\n",
    "        y_lift = ZZ(Mod(y_lift, p^(i+1)))  # Lift to next modulus\n",
    "\n",
    "    # Final result in Q_p\n",
    "    Qp_ring = Qp(p, prec)\n",
    "    x_qp = Qp_ring(x_lift)\n",
    "    y_qp = Qp_ring(y_lift)\n",
    "    return Ep([x_qp, y_qp])\n",
    "\n",
    "Fp = GF(61)\n",
    "E = EllipticCurve(Fp, [0, 2])\n",
    "P = E(1, 8)\n",
    "\n",
    "lifted = HenselLift(P, 61, 3)\n",
    "print(\"Lifted point:\", lifted.xy())"
   ]
