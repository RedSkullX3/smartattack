    "#Checking if the curve is anomalous and then printing it's points\n",
    "def analyze_custom_weierstrass(p, b1, b2, a1, a2, a3):\n",
    "    F = GF(p)\n",
    "    # Convert coefficients to field elements\n",
    "    b1, b2, a1, a2, a3 = map(F, [b1, b2, a1, a2, a3])\n",
    "    \n",
    "    try:\n",
    "        E = EllipticCurve(F, [b1, a1, b2, a2, a3])  \n",
    "    except ValueError as e:\n",
    "        raise ValueError(f\"Invalid/singular curve over F_{p}: {e}\")\n",
    "    \n",
    "    N = E.cardinality()\n",
    "    pts = E.points()\n",
    "    return N, pts, (N == p)\n",
    "\n",
    "# Example: y^2 + b1路xy + b2路y = x^3 + a1路x^2 + a2路x + a3 over F_p\n",
    "p = 23\n",
    "b1, b2 = 0, 0\n",
    "a1, a2, a3 = 7, 3, 6\n",
    "\n",
    "N, pts, is_anom = analyze_custom_weierstrass(p, b1, b2, a1, a2, a3)\n",
    "print(f\"E over F_{p}: y^2 + {b1}xy + {b2}y = x^3 + {a1}x^2 + {a2}x + {a3}\")\n",
    "print(\"  #E(F_p) =\", N)\n",
    "print(\"  Anomalous?\", is_anom)\n",
    "print(\"  Points:\", pts)\n",
    "\n",
    "\\end{lstlisting}"
   ]
