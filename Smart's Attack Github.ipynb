    "#To find valuation and p-adic absolute value\n",
    "\n",
    "def padic_analysis(a, p):\n",
    "    if not is_prime(p):\n",
    "        raise ValueError(f\"{p} is not a prime number.\")\n",
    "\n",
    "    a = QQ(a)  # ensure a is a rational number\n",
    "    v = a.valuation(p)\n",
    "    absval = p ** (-v)\n",
    "    return v, absval\n",
    "\n",
    "\n",
    "v, absval = padic_analysis(4/11, 11)\n",
    "print(\"Valuation =\", v)\n",
    "print(\"P-adic Absolute Value =\", absval)"
   ]
