    "#Searching for anomalous curves over prime p\n",
    "def find_anomalous_curves(p):\n",
    "    F = GF(p)\n",
    "    anomalous = []\n",
    "    for a in range(p):\n",
    "        for b in range(p):\n",
    "            if (4*a^3 + 27*b^2) % p == 0:\n",
    "                continue\n",
    "            E = EllipticCurve(F, [a, b])\n",
    "            if E.cardinality() == p:\n",
    "                anomalous.append((a, b, E.points()))\n",
    "    return anomalous\n",
    "\n",
    "# For small p, e.g. p = 11\n",
    "anoms = find_anomalous_curves(61)\n",
    "for a,b,pts in anoms:\n",
    "     print(\"y^2 = x^3 + %dx + %d mod %d is anomalous; points:\" % (a,b,61), pts)"
   ]
